import java.util.*;
import java.io.*;

class Solution {
    public int solution(int[] numbers) {
        ArrayList<Integer> al = new ArrayList<>();
        
        for(int n : numbers){
            al.add(n);
        }
        int sum =0;
        for(int i =0; i<10; i++){
            if(!al.contains(i)){
                sum += i;
            }
        }
        return sum;
    }
}
