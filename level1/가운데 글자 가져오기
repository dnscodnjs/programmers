class Solution {
    public String solution(String s) {
        
        String str = "";
        
        int mid = s.length()/2;
        
        if(s.length()%2 == 0){
            str = s.substring(mid-1,mid+1);
        }else{
            str = String.valueOf(s.charAt(mid));
        }
        
        return str;
    }
}
